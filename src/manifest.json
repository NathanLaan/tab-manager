{
  "name": "Tab-Manager",
  "description": "A basic tab manager that allows you to find or navigate to tabs quickly.",
  "version": "0.3.4",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "MacCtrl+Shift+E"
      },
			"description": "Tab-Manager",
			"global": false		
		}
  },
  "action": {
    "default_title": "Tab-Manager (Ctrl+Shift+E)",
    "default_icon": "/images/letter-t-square-32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "/images/letter-t-square-32.png",
    "512": "/images/letter-t-square-512.png"
  }
}
