{
  "name": "Tab-Manager",
  "description": "Manage tabs",
  "version": "0.3.3",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "MacCtrl+K"
      },
			"description": "Tab-Manager (Ctrl+K)",
			"global": false		
		}
  },
  "action": {
    "default_title": "Tab-Manager (Ctrl+K)",
    "default_icon": "/images/letter-t-square-32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "/images/letter-t-square-32.png",
    "512": "/images/letter-t-square-512.png"
  }
}
